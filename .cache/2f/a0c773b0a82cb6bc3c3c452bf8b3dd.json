umn":0}},{"source":"public/nav.css","original":{"line":261,"column":0},"generated":{"line":261,"column":0}},{"source":"public/nav.css","original":{"line":262,"column":0},"generated":{"line":262,"column":0}},{"source":"public/nav.css","original":{"line":263,"column":0},"generated":{"line":263,"column":0}},{"source":"public/nav.css","original":{"line":264,"column":0},"generated":{"line":264,"column":0}},{"source":"public/nav.css","original":{"line":265,"column":0},"generated":{"line":265,"column":0}},{"source":"public/nav.css","original":{"line":266,"column":0},"generated":{"line":266,"column":0}},{"source":"public/nav.css","original":{"line":267,"column":0},"generated":{"line":267,"column":0}},{"source":"public/nav.css","original":{"line":268,"column":0},"generated":{"line":268,"column":0}},{"source":"public/nav.css","original":{"line":269,"column":0},"generated":{"line":269,"column":0}}],"sources":{"public/nav.css":".intro-phrase {\r\n    font-size: 1em;\r\n}\r\n\r\nnav {\r\n    position: relative;\r\n    z-index: 0;\r\n    top: 0;\r\n    width: 100vw;\r\n    margin-bottom: 230px;\r\n}\r\n\r\n.nav-links>li>a {\r\n    color: var(--yellow);\r\n    text-decoration: none;\r\n    font-size: 2em;\r\n    display: inline-block;\r\n    font-weight: bold;\r\n    /* width: 100%; */\r\n}\r\n\r\n.nav-links>li:hover {\r\n    background-color: var(--light-dark-blue);\r\n}\r\n\r\nnav ul {\r\n    list-style: none;\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n.nav-links {\r\n    background-color: var(--dark-blue);\r\n    align-items: center;\r\n    justify-content: center;\r\n    display: flex;\r\n\r\n    margin: 0;\r\n    /* padding: 0px 5vw; */\r\n}\r\n\r\n.nav-links>li {\r\n\r\n    position: relative;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex: 1;\r\n    border-right: 3px solid var(--gray);\r\n    transition: all 0.2s;\r\n    padding: 0;\r\n    margin: 0;\r\n    /* height: 10vh; */\r\n    height: 65px;\r\n    /* transition: all 0.5s; */\r\n}\r\n\r\n.about-me {\r\n    display: flex;\r\n}\r\n\r\n.dropdown {\r\n    /* opacity: 0; */\r\n    position: absolute;\r\n    overflow: hidden;\r\n    padding: 20px;\r\n    padding-top: 0px;\r\n    border-radius: 2px;\r\n    transition: all 0.5s;\r\n    transform: translateY(65px);\r\n    will-change: opacity;\r\n    display: none;\r\n    z-index: 1;\r\n    margin: 0;\r\n    align-self: flex-start;\r\n    justify-self: flex-start;\r\n}\r\n\r\n.dropdown1 {\r\n    padding-top: 20px;\r\n}\r\n\r\n.trigger-enter .dropdown {\r\n    display: grid;\r\n}\r\n\r\n.trigger-enter-active .dropdown {\r\n    opacity: 1;\r\n}\r\n\r\n.dropdownBackground {\r\n    width: 100px;\r\n    height: 100px;\r\n    position: absolute;\r\n    background: var(--red);\r\n    border-radius: 4px;\r\n    box-shadow: 0 50px 100px rgba(50, 50, 93, 0.1),\r\n        0 15px 35px rgba(50, 50, 93, 0.15), 0 5px 15px rgba(0, 0, 0, 0.1);\r\n\r\n    transition: all 0.3s, opacity 0.1s, transform 0.2s;\r\n    transform-origin: 50% 0;\r\n    display: flex;\r\n    justify-content: center;\r\n    opacity: 0;\r\n    z-index: 1;\r\n}\r\n\r\n.dropdownBackground.open {\r\n    opacity: 1;\r\n}\r\n\r\n.arrow {\r\n    position: absolute;\r\n    width: 20px;\r\n    height: 20px;\r\n    display: block;\r\n    background: var(--red);\r\n    transform: translateY(-50%) rotate(45deg);\r\n}\r\n\r\n.bio {\r\n    font-size: 14px;\r\n    line-height: 1.7;\r\n    background-color: var(--gray);\r\n    box-shadow: 0 20px 35px rgba(50, 50, 93, 0.1),\r\n        0 7px 17px rgba(50, 50, 93, 0.15), 0 3px 8px rgba(0, 0, 0, 0.1);\r\n    padding: 10px;\r\n    border-radius: 5px;\r\n    transition: all 0.3s, opacity 0.1s, transform 0.2s;\r\n}\r\n\r\n.bio p {}\r\n\r\n.icon {\r\n    width: 150px;\r\n    float: left;\r\n    margin-right: 10px;\r\n    border-radius: 5px;\r\n    box-shadow: 0 10px 15px rgba(50, 50, 93, 0.1),\r\n        0 7px 17px rgba(50, 50, 93, 0.15), 0 2px 5px rgba(0, 0, 0, 0.1);\r\n    height: auto;\r\n}\r\n\r\n.projects {\r\n    padding-top: 20px;\r\n}\r\n\r\n.background {\r\n    background-color: var(--gray);\r\n    box-shadow: 0 20px 35px rgba(50, 50, 93, 0.1),\r\n        0 7px 17px rgba(50, 50, 93, 0.15), 0 3px 8px rgba(0, 0, 0, 0.1);\r\n    padding: 15px;\r\n    border-radius: 5px;\r\n    display: grid;\r\n    grid-gap: 20px;\r\n    align-content: center;\r\n    justify-content: center;\r\n    transition: all 0.5s;\r\n}\r\n\r\n.button-background {\r\n    background-color: var(--gray);\r\n    box-shadow: 0 20px 35px rgba(50, 50, 93, 0.1),\r\n        0 7px 17px rgba(50, 50, 93, 0.15), 0 3px 8px rgba(0, 0, 0, 0.1);\r\n    padding: 5px;\r\n    border-radius: 5px;\r\n    display: grid;\r\n    align-content: center;\r\n    justify-content: center;\r\n    transition: all 0.5s;\r\n}\r\n\r\n.projects li {\r\n\r\n    border-radius: 5px;\r\n    padding: 10px;\r\n    background-color: rgba(6, 133, 135, 0.7);\r\n    box-shadow: 0 10px 15px rgba(50, 50, 93, 0.1),\r\n        0 7px 17px rgba(50, 50, 93, 0.15), 0 2px 5px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.dropdown a {\r\n    text-decoration: none;\r\n    color: var(--yellow);\r\n}\r\n\r\na.button {\r\n    background: black;\r\n    display: grid;\r\n    padding: 10px;\r\n    color: white;\r\n    margin: 10px;\r\n    text-align: center;\r\n    border: none;\r\n    border-radius: 5px;\r\n    margin-top: 10px;\r\n    box-shadow: 0 10px 15px rgba(50, 50, 93, 0.1),\r\n        0 7px 17px rgba(50, 50, 93, 0.15), 0 2px 5px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.dropdown3 {\r\n    /* padding-top: var(--button-spacing); */\r\n    padding-top: 20px;\r\n}\r\n\r\n/* Matches Twitter, TWITTER, twitter, tWitter, TWiTTeR... */\r\n\r\n/* \r\n\r\n.button[href*='twitter'] {\r\n         background: #019fe9;\r\n       }\r\n       .button[href*='facebook'] {\r\n         background: #3b5998;\r\n       } \r\n*/\r\n.button[href*='in/adelgado0723'] {\r\n    background: #0077b5;\r\n}\r\n\r\n.button[href*='andy-delgado.com'] {\r\n    background: #f2b134;\r\n}\r\n\r\n/* Fixed site nav bar effect */\r\n\r\nnav {\r\n    transition: all 0.5s;\r\n    z-index: 1;\r\n}\r\n\r\nli.logo {\r\n    max-width: 0;\r\n    overflow: hidden;\r\n    font-weight: 600;\r\n    text-shadow: 3px 3px 0 var(--pale-cornflower-blue);\r\n    background-color: var(--yellow);\r\n    border: none;\r\n    /* display: none; */\r\n\r\n    font-size: 2.5em;\r\n    flex: 0;\r\n}\r\n\r\nli.logo:hover {\r\n    background-color: var(--yellow);\r\n}\r\n\r\nli.logo a {\r\n    color: var(--dark-blue);\r\n\r\n    font-family: 'Luckiest Guy';\r\n}\r\n\r\nbody.fixed-nav nav {\r\n    position: fixed;\r\n    box-shadow: 0 5px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.fixed-nav li.logo {\r\n    max-width: 500px;\r\n    flex: 1;\r\n    border-right: 3px solid var(--gray);\r\n    /* display: flex; */\r\n}\r\n\r\nbody.fixed-nav .site-wrap {\r\n    transform: scale(1);\r\n}"},"lineCount":269}},"error":null,"hash":"0a10a8c9ec4f16444f250ba74f5d9a63","cacheData":{"env":{}}}