elf: center;\r\n  padding: 10px 15px;\r\n  /* margin: 10px 0px; */\r\n  border: none;\r\n  background-color: var(--red);\r\n  font-weight: bold;\r\n  font-size: 2.5em;\r\n  font-family: 'Luckiest Guy';\r\n  text-shadow: 3px 3px 0 var(--dark-blue);\r\n  border: 5px solid var(--dark-blue);\r\n  box-shadow: 0 50px 100px rgba(50, 50, 93, 0.1),\r\n    0 15px 35px rgba(50, 50, 93, 0.15), 0 5px 15px rgba(0, 0, 0, 0.1);\r\n  color: var(--tangerine);\r\n  text-shadow: 2px 2px 0 var(--dark-blue);\r\n  border-radius: 5px;\r\n}\r\n\r\n.contact-button:active {\r\n  background-color: var(--dark-red);\r\n  text-shadow: 3px 3px 0 var(--dark-dark-blue);\r\n  border: 5px solid var(--dark-dark-blue);\r\n  color: var(--dark-tangerine);\r\n  text-shadow: 2px 2px 0 var(--dark-dark-blue);\r\n}\r\n\r\n.contact-button:disabled:hover,\r\n.contact-button:disabled:active,\r\n.contact-button:disabled {\r\n  opacity: .80;\r\n  background-color: var(--dark-red);\r\n  text-shadow: 3px 3px 0 var(--dark-dark-blue);\r\n  border: 5px solid var(--dark-dark-blue);\r\n  color: var(--dark-tangerine);\r\n  text-shadow: 2px 2px 0 var(--dark-dark-blue);\r\n\r\n\r\n}\r\n\r\n.contact-button:hover {\r\n  background-color: var(--light-red);\r\n  text-shadow: 3px 3px 0 var(--light-dark-blue);\r\n  border: 5px solid var(--light-dark-blue);\r\n  color: var(--light-tangerine);\r\n  text-shadow: 2px 2px 0 var(--light-dark-blue);\r\n\r\n}\r\n\r\n\r\n.site-wrap {\r\n  border-radius: 15px;\r\n  max-width: 75vw;\r\n  margin: 30px auto;\r\n  background: var(--background-gray);\r\n  padding: 40px;\r\n  text-align: justify;\r\n  box-shadow: 0 0 10px 5px rgba(0, 0, 0, 0.05);\r\n  transform: scale(0.98);\r\n  transition: transform 0.5s;\r\n  display: grid;\r\n  justify-content: center;\r\n  align-content: center;\r\n}\r\n\r\n.form-submit {\r\n  justify-self: center;\r\n  align-self: center;\r\n  margin: 30px;\r\n}\r\n\r\n@media screen and (max-width: 992px) {\r\n  .icon {\r\n    width: 100px;\r\n  }\r\n\r\n  .title {\r\n    font-size: 6em;\r\n  }\r\n\r\n  .contact-button {\r\n    font-size: 1em;\r\n  }\r\n\r\n  .intro-phrase {\r\n    font-size: .9em;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 600px) {\r\n  .icon {\r\n    width: 50px;\r\n  }\r\n\r\n  .title {\r\n    font-size: 3em;\r\n  }\r\n\r\n  .contact-button {\r\n    font-size: .8em;\r\n  }\r\n\r\n  .intro-phrase {\r\n    font-size: .7em;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 400px) {\r\n  .icon {\r\n    width: 30px;\r\n  }\r\n\r\n  .title {\r\n    font-size: 2em;\r\n    /* text-shadow: var(--red) 1px 1px; */\r\n  }\r\n\r\n  .contact-button {\r\n    font-size: .75em;\r\n    /* text-shadow: var(--dark-blue) 1px 1px; */\r\n    /* border: 2px solid var(--dark-blue); */\r\n  }\r\n\r\n  .intro-block {\r\n    /* border: 5px solid var(--dark-blue); */\r\n  }\r\n\r\n  .intro-phrase {\r\n    font-size: .67em;\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"e3d56f3490f6e5ce48228e3e185a8081","cacheData":{"env":{}}}