{
  "name": "Andy-Delgado.com",
  "version": "1.0.0",
  "description": "My website featuring projects that I have worked on",
  "main": "code.js",
  "scripts": {
    "start": "run-script-os",
    "start:win32": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --browser \"C:\\Program Files\\Firefox Developer Edition\\firefox.exe\"",
    "//": "Hello! If you are having trouble running this command. Try changing Firefox Developer Edition to FirefoxDeveloperEdition",
    "start:darwin:linux": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --browser 'Firefox Developer Edition'",
    "format": "./node_modules/.bin/prettier --write \"./src/**/*.{js,jsx}\"",
    "lint": "./node_modules/.bin/eslint **/*.{js,jsx} --quiet",
    "dev": "parcel ./src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adelgado0723/andy-delgado.com.git"
  },
  "keywords": [
    "javascript"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adelgado0723/andy-delgado.com/issues"
  },
  "homepage": "https://github.com/adelgado0723/andy-delgado.com#readme",
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "browser-sync": "^2.26.7",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "run-script-os": "^1.0.7"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "nodemailer": "^6.3.0",
    "normalize.css": "^8.0.1",
    "parcel": "^1.12.3"
  }
}
